pipeline {
  agent any 
  parameters { 
      choice(name: 'ENTORNOS', choices: ['dev', 'pre', 'pro'], description: 'Seleccione el entorno a utilizar')
      choice(name: 'ACCION', choices: ['', 'deployment', 'destroy'], description: 'Seleccione el entorno a utilizar')
    }
  stages {
    stage('build') {
      steps {
        sh 'pwd' 
        sh 'ls -l'
        sh 'python  deployment_dinamic.py'
      } // steps
    } // stage
    
      
  }  // stages
} //pipeline